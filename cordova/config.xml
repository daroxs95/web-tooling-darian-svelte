<?xml version='1.0' encoding='utf-8'?>
<widget id="io.daroxs95.web-tooling" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <icon src="icon.png" width="57" height="57" density="mdpi" />
    <name>Web tooling with Svelte</name>
    <description>
        Simple app
    </description>
    <author email="daroxs95@gmail.com" href="https://daroxs95.github.io">
        Darián López Utra
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <preference name="android-minSdkVersion" value="22" />
        <splash src="res/splash.png" density="land-hdpi"/>
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <splash src="res/splash.png" width="2732" height="2732"/>
    </platform>
</widget>
